    <MudStack Row="true">
       <MudStack Spacing="1">
            <MudText>Услуга: <b>@Service.Name</b></MudText>
            <MudText>План: <b>@Service.Plan</b> шт</MudText>
            <MudText>Факт:
                <MudElement @bind-Ref="StringFactRef" value="@Service.Fact" HtmlTag="input" Style="border:0; outline:0; padding:0px 1px; height:28px; width:30px; font-weight:bold" />
                шт</MudText>
            <MudText>До 85%: <b>(12+12)/2</b> шт</MudText>
            <MudText>До 100%: <b>22</b> шт</MudText>
       </MudStack>
        <MudStack Justify="Justify.Center" AlignItems="AlignItems.Center" Style="width:50px">
            <MudIconButton OnClick="FocusRowFactAsync" Icon="@Icons.Material.Outlined.Edit" Color="Color.Success" Size="Size.Small" />
       </MudStack>
    </MudStack>





