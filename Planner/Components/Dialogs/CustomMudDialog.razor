<MudDialog>
    <DialogContent>
        <MudText>@ContentText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">@NoButtonText</MudButton>
        <MudButton Color="@Color" Variant="Variant.Filled" OnClick="Submit">@OkButtonText</MudButton>
    </DialogActions>
</MudDialog>
